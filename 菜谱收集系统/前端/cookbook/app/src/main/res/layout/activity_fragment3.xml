<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment1Activity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/tabooback"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:paddingTop="20dp"
        android:paddingBottom="20dp"
        >

    </RelativeLayout>
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/activity_Linear1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="35dp"
        android:orientation="horizontal">
        <EditText
            android:layout_width="160dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:background="@drawable/textview_border"
            android:hint="  请输入食材1"
            android:ems="10"
            tools:layout_editor_absoluteY="100dp"
            tools:layout_editor_absoluteX="32dp"
            android:id="@+id/editText133"/>
        <TextView
            android:layout_width="28dp"
            android:layout_height="10dp"/>
        <EditText
            android:layout_width="160dp"
            android:layout_height="50dp"
            android:inputType="textPersonName"
            android:background="@drawable/textview_border"
            android:hint="  请输入食材2"
            android:ems="10"
            tools:layout_editor_absoluteY="79dp"
            tools:layout_editor_absoluteX="220dp"
            android:id="@+id/editText233"/>

    </LinearLayout>
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/activity_Linear2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="95dp"
        android:layout_marginLeft="180dp"
        android:orientation="horizontal">

        <Button
            android:scaleType="centerInside"
            android:background="@drawable/search"
            android:layout_width="65dp"
            android:layout_height="55dp"
            android:id="@+id/Button6"/>
    </LinearLayout>
    <ScrollView
        android:layout_marginLeft="40dp"
        android:layout_marginTop="160dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_width="326dp"
        android:layout_height="600dp">
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/activity_register"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="4dp"
        android:orientation="vertical">
        <TextView
            android:background="@drawable/textview_border"
            android:hint="在这里你将看到所查询的两种食材的相性信息~更多搭配等你发现！"
            android:layout_width="326dp"
            android:layout_height="450dp"
            android:textSize="20sp"
            android:id="@+id/TextView33"/>

    </LinearLayout>
    </ScrollView>


</android.support.constraint.ConstraintLayout>

